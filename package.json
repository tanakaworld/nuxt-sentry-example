{
  "name": "nuxt-sentry-example",
  "version": "1.0.0",
  "description": "Example project for Nuxt + Sentry",
  "author": "tanakaworld",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
